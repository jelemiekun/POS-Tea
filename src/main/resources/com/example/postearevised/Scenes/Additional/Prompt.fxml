<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="332.0" prefWidth="642.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.postearevised.Controllers.Additional.Prompt">
   <children>
      <AnchorPane prefHeight="332.0" prefWidth="642.0">
         <children>
            <Label fx:id="labelHeader" alignment="CENTER" layoutX="27.0" layoutY="42.0" prefHeight="76.0" prefWidth="588.0" text="Are you sure you want to go back?" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Arial Bold" size="33.0" />
               </font>
            </Label>
            <Label fx:id="labelContent" alignment="CENTER" layoutX="54.0" layoutY="118.0" prefHeight="56.0" prefWidth="535.0" text="Any unsaved changes will be lost!" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Microsoft Sans Serif" size="24.0" />
               </font>
            </Label>
            <AnchorPane layoutX="69.0" layoutY="209.0" prefHeight="51.0" prefWidth="506.0">
               <children>
                  <ImageView fx:id="btnLeft" fitHeight="150.0" fitWidth="200.0" layoutX="14.0" layoutY="2.0" onMouseClicked="#btnLeftClicked" onTouchReleased="#btnLeftTouched" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../Medias/Buttons/yes.png" />
                     </image>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </ImageView>
                  <ImageView fx:id="btnRight" fitHeight="150.0" fitWidth="200.0" layoutX="292.0" layoutY="2.0" onMouseClicked="#btnRightClicked" onTouchReleased="#btnRightTouched" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../Medias/Buttons/no.png" />
                     </image>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </ImageView>
                  <ImageView fx:id="btnCenter" fitHeight="150.0" fitWidth="200.0" layoutX="153.0" layoutY="2.0" onMouseClicked="#btnCenterClicked" onTouchReleased="#btnCenterTouched" pickOnBounds="true" preserveRatio="true" visible="false">
                     <image>
                        <Image url="@../../Medias/Buttons/no.png" />
                     </image>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </ImageView>
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
